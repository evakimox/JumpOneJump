            <kind>1</kind>
            <string>///state machine implementation
if(currentState == mainCharState.stand){
    //检测按钮增加向上初速度数值
    if(keyboard_check(vk_space)){
        speedY = getSmaller(speedY+0.7,maxSpeedY);
        speedX = getSmaller(speedX+0.7,maxSpeedX);
    }
    //松开按钮实现跳跃
    if(keyboard_check_released(vk_space)){
        //赋予初始速度(因为向左向上所以加负号)
        vspeed = - speedY;
        hspeed = - speedX;
        //已经起跳，蓄力置零
        speedY = 0;
        speedX = 0;
        currentState = mainCharState.air;
    }
}

if(currentState == mainCharState.air){
    //坠落
    vspeed = vspeed + mainCharGravity;
    if(instance_place(x,y+vspeed,obj_floorPlain)){
        while(!instance_place(x,y+1,obj_floorPlain)){
            y = y+1;
        }
        vspeed = 0;
        hspeed = 0;
        currentState = mainCharState.stand;
    }
}
</string>
